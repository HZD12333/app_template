{
  "name": "web-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "book:dev": "pnpm -F book dev",
    "start": "plop --plopfile ./scripts/auto-start.js",
    "clean:top": "rimraf */**/dist */dist coverage dwt*",
    "clean:modules": "rimraf node_modules **/node_modules **/**/node_modules",
    "clean:all": "pnpm clean:top && pnpm clean:modules",
    "lint:fix": "eslint --fix \"apps/**/src/**/*.{ts,tsx}\"",
    "bootstrap": "pnpm i",
    "reinstall": "pnpm clean:all && pnpm bootstrap",
    "prepare": "husky install",
    "commit": "git-cz",
    "changeset": "changeset",
    "vp": "changeset version",
    "update:version": "changeset && pnpm vp"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*{.ts,.tsx}": [
      "pnpm lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/validate-commit-msg.js"
    }
  },
  "dependencies": {
    "@miyuan/assets-retry-plugin": "^0.0.4",
    "@miyuan/constants": "^1.2.0",
    "@miyuan/enums": "^1.3.0",
    "@miyuan/helpers": "^1.4.0",
    "@miyuan/hooks": "^1.3.0",
    "@miyuan/native-apis": "^1.5.0",
    "@miyuan/service": "^1.3.0",
    "@miyuan/types": "^1.2.0",
    "@miyuan/ui-omi": "^1.3.0-beta.1",
    "@miyuan/utils": "^1.4.1",
    "@sentry/react": "^7.12.1",
    "antd-mobile": "^5.39.0",
    "axios": "^1.3.4",
    "babel-plugin-import": "^1.13.8",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "fetch-jsonp": "^1.1.3",
    "html2canvas": "^1.4.1",
    "intersection-observer": "^0.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.5.2",
    "uuid": "^8.3.2",
    "zod": "^3.25.23",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.24.7",
    "@babel/plugin-transform-typescript": "^7.22.15",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.17.12",
    "@changesets/cli": "^2.27.10",
    "@tarojs/taro": "workspace:*",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.19.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.29.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-legacy": "^5.1.1",
    "@vitejs/plugin-react": "^4.3.1",
    "chalk": "^4.1.2",
    "commitizen": "^4.3.0",
    "core-js": "^3.23.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.0",
    "jsdom": "^19.0.0",
    "less": "^4.3.0",
    "lib-flexible": "^0.3.2",
    "lint-staged": "^11.0.1",
    "plop": "^4.0.0",
    "postcss": "^8.4.18",
    "postcss-less": "^6.0.0",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "shelljs": "^0.8.5",
    "stylelint": "^15.10.1",
    "stylelint-less": "^1.0.8",
    "terser": "^5.39.0",
    "tslib": "^2.6.3",
    "typescript": "^4.7.4",
    "vite": "5.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0"
  }
}